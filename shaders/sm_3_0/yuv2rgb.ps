#include "common.h"

vector4 main (vector2 uv: TEXCOORD0): COLOR
{
	vector3 YUV = tex2D (s_base, uv);

    vector Y = YUV.x;
    vector U = YUV.y;
    vector V = YUV.z;

    vector c = 1.16406f;
	vector3 _Y = vector3 (c, c, c) * Y;
	vector3 _U = vector3 (0, -0.390625f, +2.01562f) * U;
	vector3 _V = vector3 (+1.59765f, -0.8125f, 0) * V;
	vector3 _S = vector3 (-0.86961f, +0.53076f, -1.0786f);
	

    return vector4 (_Y + _U + _V + _S, 1);
}

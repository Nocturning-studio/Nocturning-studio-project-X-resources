#include "common.h"

struct v2p
{
    vector2 tc0: TEXCOORD0;
    vector2 tc1: TEXCOORD1;
    vector4 c0: COLOR0;
};

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
vector4 main (v2p I): COLOR
{
    vector4 t_base = tex2D (s_base, I.tc0);

    return vector4 (t_base * I.c0.a);
}

#include "common.xrh"

struct v2p
{
    float2 tc0: TEXCOORD0;
    float2 tc1: TEXCOORD1;
    float4 c0: COLOR0;
};

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
float4 main (v2p I): COLOR
{
    float4 t_base = tex2D (s_base, I.tc0);

    return float4 (t_base * I.c0.a);
}

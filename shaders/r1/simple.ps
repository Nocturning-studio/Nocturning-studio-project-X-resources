#include "common.xrh"

struct v2p
{
    float2 tc0: TEXCOORD0; // base
    float fog: FOG;
};


//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
float4 main (v2p I): COLOR
{
    float4 t_base = tex2D (s_base, I.tc0);
    t_base.rgb = ApplyFogness (t_base.rgb, I.fog);
    
	// out
    return t_base;
}

#include "common.h"

struct v2p
{
    float2 tc0: TEXCOORD0; // base
    float fog: COLOR2;
};


//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
float4 main (v2p I):SV_Target
{
    float4 t_base = tex2D (s_base, I.tc0);
    t_base.rgb = ApplyFogness (t_base.rgb, I.fog);

    t_base.rgb = CalcTonemap (t_base.rgb);
    
	// out
    return t_base;
}

#include "common.h"

struct v2p
{
    float4 c: COLOR0;
    float fog: FOG;
};

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
float4 main (v2p I): COLOR
{
    float4 t_base = I.c;
    t_base.rgb = ApplyFogness (t_base.rgb, I.fog);

	// out
    return t_base;
}

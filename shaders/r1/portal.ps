#include "common.h"

struct v2p
{
    float4 c: COLOR0;
    float fog: COLOR2;
};

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
float4 main (v2p I):SV_Target
{
    float4 t_base = I.c;
    t_base.rgb = ApplyFogness (t_base.rgb, I.fog);
    
    t_base.rgb = CalcTonemap (t_base.rgb);

	// out
    return t_base;
}
